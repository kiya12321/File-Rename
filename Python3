import os
import re


def rename_files():
    # (1) Get files from folder
    file_list = os.listdir(r"C:\Users\kiya12321\Desktop\prank")
    # print(file_list)
    saved_path = os.getcwd()
    # print("Current Working Directory os " + saved_path)
    os.chdir(r"C:\Users\kiya12321\Desktop\prank")

    # (2) for each file rename filename
    for file_name in file_list:
        os.rename(file_name, re.sub("[0-9]", "", file_name))
    os.chdir(saved_path)


rename_files()
